{
  "name": "project1",
  "version": "1.0.0",
  "description": "Curso de webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --output bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./extract-text/webpack.config.js",
    "build:multiple": "webpack --config ./multiple-entries/webpack.config.js",
    "build:dev": "webpack --config ./webpack-server/webpack.config.js --watch",
    "build:serve": "webpack-dev-server --config ./webpack-server/webpack.config.js --watch",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --watch"
